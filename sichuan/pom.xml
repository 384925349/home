	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<module.COMP_Identity.src>COMP_Identity/src/main/java</module.COMP_Identity.src>
		<module.COMP_Management.src>COMP_Management/src/main/java</module.COMP_Management.src>
	</properties>
	<scm>
		<connection>scm:svn:http://10.10.125.150/svn2/COMP/COMP/src/trunk/COMP_Parent</connection>
		<developerConnection>scm:svn:http://10.10.125.150/svn2/COMP/COMP/src/trunk/COMP_Parent</developerConnection>
		<url>scm:svn:http://10.10.125.150/svn2/COMP/COMP/src/trunk/COMP_Parent</url>
	</scm>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<version>2.1</version>
				<configuration>
					<tagBase>http://10.10.125.150/svn2/COMP/COMP/src/tags/test_base/${project.name}</tagBase>
					<username>mid-ci</username>
					<password>MID@cc1222</password>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.4.3</version>
			</plugin>
		</plugins>
	</build>
