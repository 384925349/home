<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE validators PUBLIC 
  		"-//Apache Struts//XWork Validator 1.0.2//EN"
  		"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
	<field name="catalogType">
		<field-validator type="regex">
			<param name="expression"><![CDATA[^[0-9]{1,2}$]]></param>
			<message>服务目录类型只能为数字类型，请重新输入</message>
		</field-validator>
	</field>
	<field name="catalogName">
		<field-validator type="stringlength">
			<param name="maxLength">25</param>
			<message>服务目录名称最大长度为25</message>
		</field-validator>
	</field>
	<field name="description">
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<message>服务目录描述最大长度为100</message>
		</field-validator>
	</field>
	<field name="catalogId">
		<field-validator type="stringlength">
			<param name="maxLength">25</param>
			<message>服务目录ID最大长度为25</message>
		</field-validator>
	</field>
</validators>
