<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TemplateGenerator">
	<!-- oracle序列 -->
	<select id="VMTemplateGenerator" resultClass="int">
		select  SEQ_TEMPLATEID.NEXTVAL  from  dual
	</select>
	<!-- mysql序列 -->
	<select id="TemplateGeneratorMysql" resultClass="int">
		select  nextval()  from  dual
	</select>
</sqlMap>